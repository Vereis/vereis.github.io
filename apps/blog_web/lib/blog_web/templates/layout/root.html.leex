        <div id="dropdown-menu" class="sm:hidden -z-10 absolute w-full left-0 bottom-full bg-white shadow-xl transform-gpu transition-transform duration-300">
          <%= for {menu_item, link} <- [{"blank", "#"} | menu_items] do %>
            <a class="text-xs block py-5 px-4 border-b border-gray-100 text-gray-500 hover:text-gray-800 hover:bg-gray-50" href="<%= link %>">
              <%= menu_item %>
            </a>
          <% end %>
        </div>
      </div>

      <section class="p-4 max-w-6xl mx-auto">
        <%= @inner_content %>
      </section>
    </div>

    <!-- Footer -->
    <footer class="border-t border-gray-100 mt-8 p-4 flex justify-center text-xs text-gray-500 lg:border-0 lg:pb-12 md:divide-x">
      <a class="hidden md:inline-flex px-3 items-center hover:underline" href="/rss">
        RSS Feed
        <svg xmlns="http://www.w3.org/2000/svg" class="ml-1 h-3 w-3" viewBox="0 0 20 20" fill="currentColor">
          <path d="M5 3a1 1 0 000 2c5.523 0 10 4.477 10 10a1 1 0 102 0C17 8.373 11.627 3 5 3z" />
          <path d="M4 9a1 1 0 011-1 7 7 0 017 7 1 1 0 11-2 0 5 5 0 00-5-5 1 1 0 01-1-1zM3 15a2 2 0 114 0 2 2 0 01-4 0z" />
        </svg>
      </a>
      <p class="px-6">© Chris Bailey</p>
      <a class="hidden md:inline-flex px-3 items-center hover:underline" href="https://github.com/vereis/blog">
        Source Code
        <svg xmlns="http://www.w3.org/2000/svg" class="ml-1 h-3 w-3" viewBox="0 0 20 20" fill="currentColor">
          <path fill-rule="evenodd" d="M12.316 3.051a1 1 0 01.633 1.265l-4 12a1 1 0 11-1.898-.632l4-12a1 1 0 011.265-.633zM5.707 6.293a1 1 0 010 1.414L3.414 10l2.293 2.293a1 1 0 11-1.414 1.414l-3-3a1 1 0 010-1.414l3-3a1 1 0 011.414 0zm8.586 0a1 1 0 011.414 0l3 3a1 1 0 010 1.414l-3 3a1 1 0 11-1.414-1.414L16.586 10l-2.293-2.293a1 1 0 010-1.414z" clip-rule="evenodd" />
        </svg>
      </a>
    </footer>
    <script type="text/javascript" src="<%= Routes.static_path(@conn, "/js/app.js") %>"></script>
  </body>
</html>
